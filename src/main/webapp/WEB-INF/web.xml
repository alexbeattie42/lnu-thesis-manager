<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="3.0">

    <display-name>LNU Thesis Manager</display-name>

    <servlet>
        <servlet-name>LnuThesisManager</servlet-name>
        <servlet-class>
            org.springframework.web.servlet.DispatcherServlet
        </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>LnuThesisManager</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>